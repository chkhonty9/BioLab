{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "eureka": {
    "client": {
      "serviceUrl": "${DISCOVERY_SERVICE_URL:http://localhost:8761/eureka}",
      "shouldFetchRegistry": true,
      "shouldRegisterWithEureka": true
    },
    "instance": {
      "appName": "materiels-service",
      "port": 8081
    }
  },
  "spring": {
    "application": {
      "name" : "materiels-service"
    },
    "cloud": {
      "config": {
        "failFast" : true,
        "uri": "optional:configserver:${CONFIG_SERVICE_URL:http://localhost:9999}",
        "retry" : {
          "Enabled" : true
        }
        
      }
    }
  }
}
